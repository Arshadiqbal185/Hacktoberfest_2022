public class SecondMax {
    public static void main(String[] args){
        int arr[]={21,3,45,7,89};
        int fmax=0;
        int smax=0;
        fmax=arr[0];
        smax=arr[0];
        for(int i=0;i<arr.length;i++){
            if(fmax<arr[i]){
                smax=fmax;
                fmax=arr[i];
            }
            else if(smax<arr[i]){
                smax=arr[i];
            }

        }
        System.out.println("Second max "+smax);
    }
    
}
