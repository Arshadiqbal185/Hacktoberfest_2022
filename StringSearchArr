public class StringSearchArr {
        public static void main(String args[]){
String arr[]={"Ashu","Kumar","Gangwar","Sudhanshu"};
String key=null;
key="Ashu";
int index=linearSearchArr(arr,key);
if(index==-1)
    System.out.println(key+" NOt found");
    else
    System.out.println(key+" found at index "+index);
        }
public static int linearSearchArr(String[] arr,String key){
for(int i=0;i<arr.length;i++){
if(key.equals(arr[i]))
return i;
    } 
    return -1;
}
}
    

